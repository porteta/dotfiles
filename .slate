# single layout
layout single 'Terminal':REPEAT ${topright} 
layout single 'Sublime Text 2':REPEAT ${threequarter}
layout single 'Google Chrome':REPEAT ${full}
layout single 'iTunes' ${lefthalf}
layout single 'Scout' ${lefthalf}
layout single 'Spotify' ${threequarter}
layout single 'Adium' ${bottomrightquarter}
layout single 'Finder':REPEAT ${bottomleft}

bind 1:ctrl;alt;cmd layout single

# dual layout
layout dual 'Terminal':REPEAT ${1-topright} 
layout dual 'Sublime Text 2':REPEAT ${1-threequarter} 
layout dual 'Adium' ${2-bottomrightquarter}
layout dual 'Google Chrome':REPEAT ${2-lockleft}
layout dual 'iTunes' ${2-lefthalf}
layout dual 'Scout' ${2-lefthalf}
layout dual 'Spotify' ${2-threequarter}
layout dual 'Tower' ${1-bottomrightthreequarter}
layout dual 'Finder':REPEAT ${2-bottomright}
layout dual 'Evernote' ${2-toprightthreequarter}

bind 2:ctrl;alt;cmd layout dual

