#Tyler's Config

# single Aliases
alias single     0
alias dual    1

# Abstract positions
alias full move screenOriginX;screenOriginY screenSizeX;screenSizeY							
alias lefthalf move screenOriginX;screenOriginY screenSizeX/2;screenSizeY	
alias righthalf move screenOriginX+screenSizeX/2;screenOriginY screenSizeX/2;screenSizeY
alias topleft corner top-left resize:screenSizeX/2;screenSizeY/2							
alias topright corner top-right resize:screenSizeX/2;screenSizeY/2							
alias bottomleft corner bottom-left resize:screenSizeX/2;screenSizeY/2						
alias bottomright corner bottom-right resize:screenSizeX/2;screenSizeY/2
alias bottomrightquarter corner bottom-right resize:screenSizeX/4;screenSizeY/2					
alias threequarter move screenOriginX;screenOriginY screenSizeX/1.14;screenSizeY			



#concrete positions
alias 1-full move screenOriginX;screenOriginY screenSizeX;screenSizeY							${single}
alias 1-lefthalf move screenOriginX;screenOriginY screenSizeX/2;screenSizeY						${single}
alias 1-righthalf move screenOriginX+screenSizeX/2;screenOriginY screenSizeX/2;screenSizeY		${single}
alias 1-topleft corner top-left resize:screenSizeX/2;screenSizeY/2								${single}
alias 1-topright corner top-right resize:screenSizeX/2;screenSizeY/2							${single}
alias 1-bottomleft corner bottom-left resize:screenSizeX/2;screenSizeY/2						${single}
alias 1-bottomright corner bottom-right resize:screenSizeX/2;screenSizeY/2						${single}
alias 1-bottomrightquarter corner bottom-right resize:screenSizeX/4;screenSizeY/2				${single}
alias 1-threequarter move screenOriginX;screenOriginY screenSizeX/1.14;screenSizeY				${single}
alias 1-right-threequarter corner bottom-right resize:screenSizeX/1.14;screenSizeY/1.14			${single}
alias 1-bottomrightthreequarter corner bottom-right screenSizeX/1.05;screenSizeY/1.05			${single}


alias 2-full move screenOriginX;screenOriginY screenSizeX;screenSizeY
alias 2-lefthalf move screenOriginX;screenOriginY screenSizeX/2;screenSizeY						${dual}
alias 2-righthalf move screenOriginX+screenSizeX/2;screenOriginY screenSizeX/2;screenSizeY		${dual}
alias 2-topleft corner top-left resize:screenSizeX/2;screenSizeY/2								${dual}
alias 2-topright corner top-right resize:screenSizeX/2;screenSizeY/2							${dual}
alias 2-bottomleft corner bottom-left resize:screenSizeX/2;screenSizeY/2						${dual}
alias 2-bottomright corner bottom-right resize:screenSizeX/2;screenSizeY/2						${dual}
alias 2-bottomrightquarter corner bottom-right resize:screenSizeX/4;screenSizeY/2				${dual}
alias 2-threequarter move screenOriginX;screenOriginY screenSizeX/1.14;screenSizeY/1.14			${dual}
alias 2-lockleft move screenOriginX;screenOriginY screenSizeX/1.05;screenSizeY				${dual}
alias 2-lockright corner top-right screenSizeX/1.05;screenSizeY									${dual}
alias 2-toprightthreequarter corner top-right screenSizeX/1.05;screenSizeY/1.05					${dual}

# Resize Bindings
bind right:ctrl;alt;cmd       resize +10% +0
bind right:ctrl;cmd           resize -10% +0 top-right
bind left:ctrl;alt;cmd        resize -10% +0
bind left:ctrl;cmd            resize +10% +0 top-right
bind up:ctrl;alt;cmd          resize +0   -10%
bind up:ctrl;cmd              resize +0   +10% bottom-right
bind down:ctrl;alt;cmd        resize +0   +10%
bind down:ctrl;cmd            resize +0   -10% bottom-right

# Throw Bindings
bind right:shift;alt;cmd throw right
bind left:shift;alt;cmd  throw left

#position bindings
bind 7:ctrl;alt;cmd ${lefthalf}
bind ;:ctrl;alt;cmd ${lefthalf}

bind 9:ctrl;alt;cmd ${righthalf}
bind ':ctrl;alt;cmd ${righthalf}

bind 3:ctrl;alt;cmd ${threequarter}
bind return:ctrl;alt;cmd ${threequarter}

bind f:ctrl;alt;cmd ${full}

bind pad7:ctrl;alt;cmd ${lefthalf}
bind pad9:ctrl;alt;cmd ${righthalf}
bind pad3:ctrl;alt;cmd ${threequarter}


                                                                                                                       
                                                                                                                       
